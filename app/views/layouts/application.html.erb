      <a class="navbar-brand" href="/">
        <img class="img-responsive center" src="//cdn2.hubspot.net/hubfs/514770/img/strap-logo2.png?t=1462536216307" alt="Strap">
      </a>
    </div>
    <ul class="right">
      <li><a href="#" class="notice" style="color:orange;"><%= notice %></a></li>
      <li><a href="#" class="alert" style="color:red;"><%= alert %></a></li>
      <% if user_signed_in? %>
        <li><%= link_to('SIGN OUT', destroy_user_session_path, :method => :delete) %></li>
      <% else %>
        <li><%= link_to('SIGN IN', new_user_session_path) %></li>
        <li><%= link_to('SIGN UP', new_user_registration_path) %></li>
      <% end %>
    </ul>
